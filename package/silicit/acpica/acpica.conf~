
custmain=custmain

custmain() {   
   ( cd compiler ; make )
   ( cd tools/acpisrc ; make )
   ( cd tools/acpixtract ; make )
#   ( cd tools/acpiexec ; make ) ???
echo   install -m 755 compiler/iasl $root$prefix
echo   install -m 755 tools/acpisrc/acpisrc $root$prefix
echo   install -m 755 tools/acpixtract/acpixtract $root$prefix
}
