pkgfilter sed -e '
   / e17 / { p; d; };
   / lua / { p; d; };    # edje makes use of it now
   / taglib / { p; d; }; # a dep of eclair
   / jed / { p; d; };    # a nice to have for me
   
   / CORE / ! s/^X/O/;
   / compiz / s/^X/O/;
   / kde / s/^X/O/;
   '

pkgdisable kaffeine
pkgdisable dhcp

ROCKCFGSET_DO_REBUILD_STAGE=0
ROCKCFGSET_SPLIT_DOC=0
ROCKCFGSET_SPLIT_DEV=0
	
ROCKCFGSET_PKG_PERL5_THREADS=1

ROCKCFGSET_DEFAULT_MTA=exim
pkgenable exim
pkgdisable postfix
